{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "allowImportExportEverywhere": false,
    "sourceType": "module"
  },
  "rules": {
    "semi": [0],
    "react/jsx-filename-extension": [0]
  }
}
